{
    "env": {
      "browser": true,
      "node": true,
      "commonjs": true
    },
    "extends": "airbnb",
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "comma-dangle": "off",
      "no-console": "on",
      "no-plusplus": "off",
      "semi": ["error", "never"],
      "func-names": ["error", "as-needed"],
      "arrow-parens": "off",
      "prefer-promise-reject-errors": "off",
      "max-classes-per-file": ["error", 5]
    }
  }
  
